::[nesting] @root::
#0:
	new n = $1
#1:
	%t = n < $15
	cj #2
	halt
#2:
	n = n + $1
	new i = $2
	%1 = fun high_approx_sqrt n
	new j = %1
#3:
	%t = i < j
	cj #4
	jmp #7
#4:
	%t = n % i
	cj #5
	jmp #6
#5:
	i = i + $1
	jmp #3
#6:
	i = $16
	jmp #3
#7:
	%t = i != $16
	cj #8
	jmp #1
#8:
	hardware call out n
	jmp #1

::[leaf] high_approx_sqrt m::
#0:
	%0 = m + $1
	%1 = %0 >> $1
	new a = %1
	new b
	b = m / a
	%2 = b + a
	a = %2 >> $1
	b = m / a
	%3 = b + a
	a = %3 >> $1
	b = m / a
	%4 = b + a
	a = %4 >> $1
	%5 = a + $1
	ret %5
